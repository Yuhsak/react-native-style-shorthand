  "author": "Yuhsak Inoue",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/runtime": "^7.9.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.17",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-native": "^0.61.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
