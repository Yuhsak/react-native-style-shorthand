{
  "name": "react-native-style-shorthand",
  "version": "0.60.0",
  "description": "Small set of utilities which makes you never be tired of styling components.",
  "files": [
    "lib",
    "src",
    "tsconfig.json",
    "tsconfig.esm.json"
  ],
  "main": "lib/common/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
